<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="cosmoTextVerySmall" format="reference" />
    <attr name="cosmoTextSmall" format="reference" />
    <attr name="cosmoTextMedium" format="reference" />
    <attr name="cosmoTextLarge" format="reference" />
    <attr name="cosmoTextLarger" format="reference" />
    <attr name="cosmoTextVeryLarge" format="reference" />
    <attr name="cosmoTextHuge" format="reference" />
    <attr name="cosmoTextEnormous" format="reference" />
    <attr name="cosmoTextGiant" format="reference" />

    <declare-styleable name="CosmoLoader">
        <attr name="message" format="string" />
        <attr name="speed" format="integer" />
        <attr name="maxOffset" format="float" />
    </declare-styleable>


    <declare-styleable name="CosmoRadioButton">
        <attr name="cosmoRadioButtonTitle" format="string" />
        <attr name="cosmoRadioButtonSubTitle" format="string" />
        <attr name="cosmoRadioButtonTitleTextSize" format="dimension" />
        <attr name="cosmoRadioButtonSubtitleTextSize" format="dimension" />
        <attr name="cosmoRadioButtonBackgroundHeight" format="dimension" />
        <attr name="cosmoRadioButtonBackgroundRadius" format="dimension" />
        <attr name="cosmoRadioButtonTitleTextColor" format="color" />
        <attr name="cosmoRadioButtonSubtitleTextColor" format="color" />
        <attr name="cosmoRadioButtonBackgroundColor" format="reference" />
        <attr name="cosmoRadioButtonRadioButtonColor" format="color" />
        <attr name="cosmoRadioButtonChecked" format="boolean" />
        <attr name="cosmoRadioButtonPaddingStart" format="dimension" />
        <attr name="cosmoRadioButtonPaddingEnd" format="dimension" />
        <attr name="cosmoRadioButtonSelectedDrawable" format="reference" />
        <attr name="cosmoRadioButtonUnselectedDrawable" format="reference" />
        <attr name="cosmoRadioButtonFont" format="reference"/>
    </declare-styleable>


</resources>